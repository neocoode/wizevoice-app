{
  "name": "wizevoiceapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "cp envs/.env.local .env && react-native run-ios ",
    "ios:16ProMax": "yarn kill:ports && rm -rf node_modules yarn.lock && yarn cache clean --all && yarn install && cp envs/.env.local .env && yarn preinstall:ios && yarn react-native run-ios --simulator=\"iPhone 16 Pro Max\"",
    "ios:dev": "cp envs/.env.local .env && react-native run-ios --device",
    "ios:prd": "cp envs/.env.prd .env && react-native run-ios --device",
    "ios:release": "cp envs/.env.prd .env && cd ios && xcodebuild -workspace wizevoiceapp.xcworkspace -scheme wizevoiceapp -configuration Release -sdk iphoneos build && cd ..",
    "android:dev": "cp envs/.env.dev .env && react-native run-android",
    "android:local": "cp envs/.env.local .env && react-native run-android",
    "android:prd": "cp envs/.env.prd .env && react-native run-android",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:ios": "xcrun simctl uninstall booted com.wizevoiceapp",
    "clean:android": "adb uninstall com.wizevoiceapp",
    "clean:all": "yarn clean:ios && yarn clean:android",
    "reinstall": "rm -rf node_modules yarn.lock package-lock.json && yarn install && cd ios && pod install && cd ..",
    "preinstall:ios": "cd ios && pod deintegrate && pod install && cd ..",
    "kill:ports": "lsof -i :8081 | grep LISTEN | awk '{print $2}' | xargs kill -9"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/voice": "^1.1.9",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.9.0",
    "expo-font": "^13.3.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-audio-recorder-player": "^3.6.14",
    "react-native-dotenv": "^3.4.11",
    "react-native-fs": "^2.20.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
